{
    "name": "Let's Do Delivery",
    "description": "Onboarding platform for drivers",
    "website": "https://www.delivery.com/",
    "repository": "https://github.com/kolbykskk/texas-togo-driver-portal",
    "logo": "https://via.placeholder.com/150",
    "env": {
        "CHECKR_PUBLISHABLE": {
            "description": "Checkr publishable API key"
        },
        "CHECKR_SECRET": {
            "description": "Checkr secret API key"
        },
        "PUBLISHABLE_KEY": {
            "description": "Stripe publishable API key"
        },
        "REDIS_PROVIDER": {
            "value": "REDISTOGO_URL"
        },
        "S3_BUCKET_NAME": {
            "description": "AWS S3 bucket name",
            "value": "driver-portal"
        },
        "S3_KEY": {
            "description": "AWS S3 bucket key"
        },
        "S3_REGION": {
            "description": "AWS S3 bucket region",
            "value": "us-east-2"
        },
        "S3_SECRET": {
            "description": "AWS S3 bucket secret"
        },
        "SECRET_KEY": {
            "description": "Stripe secret API key"
        },
        "TWILIO_ACCOUNT_SID": {

        },
        "TWILIO_AUTH_TOKEN": {

        },
        "TWILIO_PHONE_NUMBER": {
            
        }
    },
    "formation": {
        "web": {
            "quantity": 1,
            "size": "standard-1x"
        },
        "worker": {
            "quantity": 1,
            "size": "standard-1x"
        }
    },
    "addons": [
        "redistogo",
        "papertrail",
        "scheduler",
        "adminium"
    ]
}
