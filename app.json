{
  "addons": [
    "papertrail",
    "heroku-postgresql",
    "redistogo"
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ],
  "env": {
    "LANG": {
      "required": true
    },
    "PAPERTRAIL_API_TOKEN": {
      "required": true
    },
    "PUBLISHABLE_KEY": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "RAILS_LOG_TO_STDOUT": {
      "required": true
    },
    "RAILS_SERVE_STATIC_FILES": {
      "required": true
    },
    "REDIS_PROVIDER": {
      "required": true
    },
    "REDISTOGO_URL": {
      "required": true
    },
    "S3_BUCKET_NAME": {
      "required": true
    },
    "S3_KEY": {
      "required": true
    },
    "S3_REGION": {
      "required": true
    },
    "S3_SECRET": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true
    },
    "SECRET_KEY_BASE": {
      "required": true
    }
  },
  "formation": {
    "worker": {
      "quantity": 1
    },
    "web": {
      "quantity": 1
    }
  },
  "name": "texas-togo-driver-portal",
  "scripts": {
  },
  "stack": "heroku-18"
}
